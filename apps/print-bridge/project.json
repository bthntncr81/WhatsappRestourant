{
  "name": "print-bridge",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/print-bridge/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/print-bridge",
        "command": "pnpm tsc"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/print-bridge",
        "command": "npx ts-node src/main.ts"
      }
    }
  }
}



{
  "name": "@whatres/source",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "start:worker": "nx serve worker",
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build:worker": "nx build worker",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,html,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,html,css,scss}\"",
    "db:migrate": "cd apps/api && npx prisma migrate dev",
    "db:generate": "cd apps/api && npx prisma generate",
    "db:seed": "cd apps/api && npx ts-node prisma/seed.ts",
    "db:studio": "cd apps/api && npx prisma studio",
    "db:reset": "cd apps/api && npx prisma migrate reset --force"
  },
